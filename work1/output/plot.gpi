set term png
set output '1-1-40.png'
set encoding utf8

set xlabel "n (tamanho do vetor)"
set ylabel "Tempo (ns)"
set xtics nomirror
set ytics nomirror

set key Left 
set key left top

t(x) = a*x**2 + b*x + c
fit t(x) '1-1-40.dat' using 1:2 via a, b, c

titulo = sprintf('T(n) = %.4gn**2 + %.4gn + %.4g',a,b,c)
titulo = sprintf("MÃ©todo da bolha com vetor em ordem decrescente\n%s", titulo)
set title titulo

plot '1-1-40.dat' title 'Medicoes' with points, t(x) title 'T(n)' lt 1 lc 3